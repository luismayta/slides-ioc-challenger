{
  "keywords": [],
  "name": "@luismayta/slides-ioc-challenger",
  "description": "slides-ioc-challenger for zsh",
  "private": true,
  "repository": "ssh://git@github.com/luismayta/slides-ioc-challenger.git",
  "author": "Luis Mayta <slovacus@gmail.com>",
  "license": "MIT",
  "stylintrc": ".stylintrc",
  "version": "0.0.0",
  "scripts": {
    "generate": "cross-env TS_NODE_PROJECT='./tools/generators/ts-node.tsconfig.json' plop --plopfile tools/generators/index.ts",
    "start": "yarn clean && gatsby develop",
    "build": "./node_modules/.bin/tsc -p",
    "build:slides": "yarn clean && gatsby build --prefix-path",
    "build:tool": "yarn run build ./tools/",
    "clean": "rimraf -rf dist",
    "clean:cache": "yarn clean ./node_modules/.cache",
    "clean:build": "yarn clean dist .next out .serverless_nextjs",
    "clean:build:test": "yarn clean ./coverage",
    "clean:build:tool": "yarn clean ./build/tools",
    "clean:build:all": "yarn clean && yarn clean:build && yarn clean:build:tool && yarn clean:build:test",
    "clean:all": "yarn clean:cache && yarn clean:build:all && yarn clean node_modules",
    "lint": "eslint -c ./.eslintrc.js ./ --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:less": "stylelint --config=.stylintrc --syntax less **/*.less",
    "lint:less:fix": "yarn lint:less --fix",
    "prettier:lint": "prettier '**/*.{js?(on),ts?(x),scss,md,y?(a)ml,graphql,less}' --write=false --list-different",
    "prettier:fix": "yarn prettier:lint --write=true",
    "precommit": "lint-staged"
  },
  "publishConfig": {
    "access": "private"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@ant-design/colors": "^4.0.5",
    "@babel/core": "^7.11.6",
    "@types/react-dom": "^16.9.8",
    "@types/react": "^16.9.50",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@equipindustry/eslint-config": "^0.3.4",
    "@equipindustry/prettierrc": "^0.2.0",
    "@types/jest": "^26.0.14",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint": "==6.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gatsby-plugin-compile-es6-packages": "^2.1.0",
    "gatsby": "^2.24.67",
    "axios": "^0.20.0",
    "babel": "^6.23.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "node-plop": "^0.26.2",
    "plop": "==2.6.0",
    "prettier": "^2.1.2",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "antd": "^4.6.6",
    "mdx-deck": "^4.1.1",
    "react-dom": "^16.13.1",
    "react": "^16.13.1"
  }
}
